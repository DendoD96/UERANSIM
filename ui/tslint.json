{
  "defaultSeverity": "warning",
  "extends": ["tslint-config-airbnb", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "jsx-no-lambda": false,
    "prefer-template": false,
    "object-literal-shorthand": false,
    "no-increment-decrement": false
  },
  "rulesDirectory": []
}
